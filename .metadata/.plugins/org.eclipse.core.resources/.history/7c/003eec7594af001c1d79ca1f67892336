package Graphs;

import java.util.LinkedList;

public class Vertex {

	private int value;
	private String name;
	private LinkedList connectedNodes;
	
	public int getValue() {
		return value;
	}
	public void setValue(int value) {
		this.value = value;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public LinkedList getConnectedNodes() {
		return connectedNodes;
	}
	public void setConnectedNodes(LinkedList connectedNodes) {
		this.connectedNodes = connectedNodes;
	}
	
	
	public Vertex(int value, String name, LinkedList connectedNodes) {
		super();
		this.value = value;
		this.name = name;
		this.connectedNodes = connectedNodes;
	}
	
	
	

}

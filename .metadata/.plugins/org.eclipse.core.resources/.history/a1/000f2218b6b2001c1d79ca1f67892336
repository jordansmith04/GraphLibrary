package com.Jordan.GraphLibrary;

import com.Jordan.GraphLibrary.Models.Graph;
import com.Jordan.GraphLibrary.Models.GraphInterface;

/**
 * Factory for creating instances of {@link Graph}.
 */
public final class GraphFactory {

	private GraphFactory() {

	}

	/**
	 * Creates an instance of {@link Graph}.
	 *
	 * @return the new instance
	 */

	public static GraphInterface createGraph(T type) {

		return new Graph(type);

	}

}

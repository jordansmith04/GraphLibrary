package Graphs;

import java.util.List;
import java.util.Map;

public interface Graph<T> {
	
	void addEdge(Map<T, List<T>> graphList, T source, T destination, boolean bidirectional);
	
	void addVertex(Map<T, List<T>> graphList, T edge);
	
	void getPath(Map<T, List<T>> graphList, T source, T destination);
}
